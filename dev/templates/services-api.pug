            .cases-text
              h6 Title
              span.transparent-text Short step description
          li.cases-item
            .diagram
              .pieBackground
                .pieField
              .wrapper
                .object
                  .round
                .object.rotate
                  .round
                .pieSlice1.hold
                  .pie
                .pieSlice2.hold
                  .pie
              .diagram-inner 90
            .cases-text
              h6 Title
              span.transparent-text Short step description
          li.cases-item
            .diagram
              .pieBackground
                .pieField
              .wrapper
